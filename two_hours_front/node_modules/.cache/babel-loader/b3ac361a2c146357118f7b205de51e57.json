{"ast":null,"code":"import \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.replace.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport UserAvatar from \"@/components/UserAvatar\";\nimport WhiteLogo from \"@/components/WhiteLogo\";\nimport PageFooter from \"@/components/PageFooter\";\nexport default {\n  name: \"Management\",\n  components: {\n    UserAvatar: UserAvatar,\n    WhiteLogo: WhiteLogo,\n    PageFooter: PageFooter\n  },\n  // 数据模型\n  data: function data() {\n    return {\n      user: {\n        username: '',\n        userAvatar: ''\n      },\n      // 课程信息\n      course: {\n        coverImg: ''\n      },\n      // 顶部导航栏激活项\n      topActiveIndex: '',\n      // 激活的导航栏项\n      sideActiveIndex: '',\n      // 头像大小\n      size: 'large',\n      // 图像适应\n      coverImgFit: 'cover',\n      // 选中选项卡的序号\n      tabName: ''\n    };\n  },\n  // 启动方法\n  created: function created() {\n    this.openLoading();\n    this.getUser();\n    this.getCourse(); // 激活课程管理页\n\n    this.editCourseInfo();\n  },\n  // 方法模型\n  methods: {\n    /* 加载动画 */\n    openLoading: function openLoading() {\n      var loading = this.$loading({\n        lock: true,\n        text: 'Loading',\n        spinner: 'el-icon-loading',\n        background: 'rgba(255,255,255,0.8)'\n      });\n      setTimeout(function () {\n        loading.close();\n      }, 400);\n    },\n    // 返回主页\n    goBack: function goBack() {\n      this.$router.go(-1);\n    },\n    // 获取用户数据\n    getUser: function getUser() {\n      var _this = this;\n\n      this.$axios.get(\"/Users/isUserLogin\").then(function (res) {\n        if (res.data.flag) {\n          // 保存用户数据\n          _this.user.username = res.data.data.username;\n          _this.user.userAvatar = res.data.data.userAvatar;\n        } else {\n          _this.$router.replace({\n            name: 'Login'\n          });\n        }\n      });\n    },\n    // 获取课程信息\n    getCourse: function getCourse() {\n      var _this2 = this;\n\n      this.$axios.get(\"/CourseView/getCourseInfo\").then(function (res) {\n        if (res.data.flag) {\n          // 保存用户数据\n          _this2.course = res.data.data;\n        } else {\n          // 服务故障\n          _this2.$message.error(\"数据拉取失败！\");\n\n          _this2.$router.replace({\n            name: 'UserMainPage'\n          });\n        }\n      });\n    },\n    // 顶部导航栏激活事件\n    topNavigationBarSelect: function topNavigationBarSelect(key) {\n      var _this3 = this;\n\n      this.topActiveIndex = key;\n\n      switch (key) {\n        case '0':\n          // logo\n          this.$router.push({\n            name: 'MainPage'\n          });\n          return;\n\n        case '2':\n          // 返回我的课程\n          this.$router.replace({\n            name: 'UserMainPage'\n          });\n          return;\n\n        case '3':\n          // 退出登陆\n          this.$axios.get(\"/LoginCon/loginOut\").then(function (res) {\n            if (res.data.flag) {\n              // session清理\n              _this3.$router.replace({\n                name: 'Login'\n              });\n            } else {\n              // 服务故障\n              _this3.$message.error(\"操作失败！\");\n            }\n          });\n          return;\n      }\n    },\n    // 侧边导航栏激活事件\n    sideNavigationBarSelect: function sideNavigationBarSelect(key) {\n      this.sideActiveIndex = key;\n\n      switch (key) {\n        case '1':\n          // 章节\n          this.tabName = '章节';\n          this.$router.replace({\n            name: 'ChapterTab'\n          });\n          return;\n\n        case '2':\n          // 通知\n          this.tabName = '通知';\n          this.$router.replace({\n            name: 'NoticeTab'\n          });\n          return;\n\n        case '3':\n          // 资料\n          this.tabName = '资料';\n          this.$router.replace({\n            name: 'FileTab'\n          });\n          return;\n\n        case '4':\n          // 讨论\n          this.tabName = '讨论';\n          this.$router.replace({\n            name: 'DiscussionTab'\n          });\n          return;\n\n        case '6':\n          // 实验\n          this.tabName = '实验';\n          this.$router.replace({\n            name: 'ExperimentTab'\n          });\n          return;\n\n        case '7':\n          // 考试\n          this.tabName = '考试';\n          this.$router.replace({\n            name: 'ExamTab'\n          });\n          return;\n\n        case '8':\n          // 设置\n          this.tabName = '用户管理';\n          this.$router.replace({\n            name: 'UserManage',\n            params: {\n              courseId: this.course.courseId\n            }\n          });\n          return;\n      }\n    },\n    // 课程管理激活\n    editCourseInfo: function editCourseInfo() {\n      this.tabName = '欢迎访问教师端';\n      this.$router.replace({\n        name: 'CourseTab'\n      });\n    },\n    // 刷新课程数据-课程管理页更新数据后父页同步\n    refreshCourseInfo: function refreshCourseInfo() {\n      this.getCourse();\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgIA;AACA;AACA;AACA;AACAA,oBADA;AAEAC;AACAC,0BADA;AAEAC,wBAFA;AAGAC;AAHA,GAFA;AAOA;AACAC,MARA,kBAQA;AACA;AACAC;AACAC,oBADA;AAEAC;AAFA,OADA;AAKA;AACAC;AACAC;AADA,OANA;AASA;AACAC,wBAVA;AAWA;AACAC,yBAZA;AAaA;AACAC,mBAdA;AAeA;AACAC,0BAhBA;AAiBA;AACAC;AAlBA;AAoBA,GA7BA;AA8BA;AACAC,SA/BA,qBA+BA;AACA;AACA;AACA,qBAHA,CAIA;;AACA;AACA,GArCA;AAsCA;AACAC;AACA;AACAC,eAFA,yBAEA;AACA;AACAC,kBADA;AAEAC,uBAFA;AAGAC,kCAH